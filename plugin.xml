<plugin name='ueditor' version='1.4.3' grailsVersion='2.0 &gt; *'>
  <author>Hai-Hua Xiao</author>
  <authorEmail>hhxiao@gmail.com</authorEmail>
  <title>Grails UEditor Plugin</title>
  <description>UEditor web WYSIWYG editor integration plugin.
</description>
  <documentation>http://grails.org/plugin/grails-ueditor</documentation>
  <type>UeditorGrailsPlugin</type>
  <resources>
    <resource>UeditorUrlMappings</resource>
    <resource>org.grails.plugin.ueditor.UeditorHandlerController</resource>
    <resource>org.grails.plugin.ueditor.UeditorConfigService</resource>
    <resource>org.grails.plugin.ueditor.UeditorTagLib</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies />
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>